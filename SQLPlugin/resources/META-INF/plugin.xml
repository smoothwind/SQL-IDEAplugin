<idea-plugin>
  <id>com</id>
  <name>sql</name>
  <version>1.0</version>
  <vendor email="itbuddy@163.com" url="">Nan</vendor>

  <description><![CDATA[
      Dev assist tools for sql, Format, Inspect, Highlight, SBC case detect.<br>
      <em>SQL</em>
    ]]></description>

  <change-notes><![CDATA[
      0.1 beta .<br>
